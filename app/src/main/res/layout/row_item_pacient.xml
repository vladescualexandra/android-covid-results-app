<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/grey"
    >

    <ImageView
        android:id="@+id/vladescu_alexandra_bianca_pacient_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:src="@drawable/ic_baseline_person_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/vladescu_alexandra_bianca_pacient_nume"
        style="@style/text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/vladescu_alexandra_bianca_default"
        android:textColor="@color/blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/vladescu_alexandra_bianca_pacient_icon"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/vladescu_alexandra_bianca_pacient_telefon"
        style="@style/text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/vladescu_alexandra_bianca_default"
        android:textColor="@color/blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/vladescu_alexandra_bianca_pacient_icon"
        app:layout_constraintTop_toBottomOf="@+id/vladescu_alexandra_bianca_pacient_nume" />

    <TextView
        android:id="@+id/vladescu_alexandra_bianca_pacient_cnp"
        style="@style/text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/vladescu_alexandra_bianca_default"
        android:textColor="@color/blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/vladescu_alexandra_bianca_pacient_icon"
        app:layout_constraintTop_toBottomOf="@+id/vladescu_alexandra_bianca_pacient_telefon" />


    <TextView
        android:id="@+id/vladescu_alexandra_bianca_pacient_rezultat"
        style="@style/text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:text="@string/vladescu_alexandra_bianca_default"
        android:textColor="@color/blue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/vladescu_alexandra_bianca_pacient_icon"
        app:layout_constraintTop_toBottomOf="@+id/vladescu_alexandra_bianca_pacient_cnp" />

</androidx.constraintlayout.widget.ConstraintLayout>